Options -MultiViews
RewriteEngine On

RewriteBase /

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-l

# this takes any url and redirects to index.php?<somepath>, this way we can get that as router data
RewriteRule ^(.+)$ index.php?url=$1 [QSA,L]
